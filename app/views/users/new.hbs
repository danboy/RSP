{{#properties}}
  "page_title": "Create a reservation."
{{/properties}}

<form action="/users" method="post">
  <fieldset>
    <legend>Sign up</legend>
    <div>
      <label for="editUserName">Name</label>
      <input type="text" name="user[name]"/>
    </div>
    <div>
      <label for="editUserName">Password</label>
      <input type="text" name="user[pass]"/>
    </div>
    <div id="editUserSubmit" class="submit">
      <input type="submit" />
    </div>
  </fieldset>
</form>
